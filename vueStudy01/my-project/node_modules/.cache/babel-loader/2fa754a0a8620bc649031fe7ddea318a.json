{"ast":null,"code":"export default {\n  name: \"user\",\n\n  data() {\n    return {\n      dataUser: {\n        msg: 'Hello User!',\n        params: {},\n        query: {}\n      },\n      showData: false\n    };\n  },\n\n  mounted() {\n    this.dataUser.params = this.$route.params;\n    this.dataUser.query = this.$route.query;\n    console.log(\"m-p\", this.$route.params);\n    console.log(\"m-q\", this.$route.query);\n    console.log(\"m\", this.dataUser);\n    console.log(\"m-this\", this);\n  },\n\n  updated() {\n    console.log(\"up\", this.dataUser);\n    console.log(\"up-this\", this.$data);\n  },\n\n  methods: {\n    showView() {\n      this.showData = true;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkBA,eAAe;AACbA,MAAI,EAAE,MADO;;AAEbC,MAAG,GAAK;AACN,WAAO;AACLC,cAAQ,EAAE;AACRC,WAAG,EAAE,aADG;AAERC,cAAM,EAAE,EAFA;AAGRC,aAAK,EAAE;AAHC,OADL;AAMLC,cAAQ,EAAE;AANL,KAAP;AAQD,GAXY;;AAabC,SAAO,GAAE;AACP,SAAKL,QAAL,CAAcE,MAAd,GAAuB,KAAKI,MAAL,CAAYJ,MAAnC;AACA,SAAKF,QAAL,CAAcG,KAAd,GAAsB,KAAKG,MAAL,CAAYH,KAAlC;AACAI,WAAO,CAACC,GAAR,CAAY,KAAZ,EAAkB,KAAKF,MAAL,CAAYJ,MAA9B;AACAK,WAAO,CAACC,GAAR,CAAY,KAAZ,EAAkB,KAAKF,MAAL,CAAYH,KAA9B;AACAI,WAAO,CAACC,GAAR,CAAY,GAAZ,EAAgB,KAAKR,QAArB;AACAO,WAAO,CAACC,GAAR,CAAY,QAAZ,EAAqB,IAArB;AACD,GApBY;;AAsBbC,SAAO,GAAE;AACPF,WAAO,CAACC,GAAR,CAAY,IAAZ,EAAiB,KAAKR,QAAtB;AACAO,WAAO,CAACC,GAAR,CAAY,SAAZ,EAAsB,KAAKE,KAA3B;AACD,GAzBY;;AA2BbC,SAAO,EAAC;AACNC,YAAQ,GAAE;AACR,WAAKR,QAAL,GAAgB,IAAhB;AACF;;AAHM;AA3BK,CAAf","names":["name","data","dataUser","msg","params","query","showData","mounted","$route","console","log","updated","$data","methods","showView"],"sourceRoot":"","sources":["/Users/fuuujy/study/vueStudy01/my-project/src/components/User.vue"],"sourcesContent":["<template>\n  <div class=\"user\">\n      <h1>{{dataUser.msg}}</h1>\n<!--      这里可以通过$route.params.name来获取路由的参数-->\n      <p v-if=\"$route.params.id\">(param)欢迎回来,{{$route.params.name}}</p>\n      <p v-if=\"$route.query.id\">（query)欢迎回来,{{$route.query.name}}</p>\n\n  